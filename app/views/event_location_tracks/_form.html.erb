<%= form_for :event_location_track, url: event_location_tracks_path, method: 'post', remote: true, html: {id: 'event-location-track-form'} do |f| %>
<h5>Create Track</h5>
<small>Select a name and create a track to add speakers to that track</small>
<div>
  <div class="class="form-group"">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.submit "Create Track", class: 'btn btn-outline-info' %>
  </div>


</div>

<% end %>