<%= form_for @kommunity do |f| %>

<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, required: true, class: 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :about %>
  <%= f.text_area :about, :class => "tinymce editor", :rows => 3, :cols => 50 %>
</div>

<div class="form-group">

  <%= f.submit 'Submit', class: 'btn btn-primary' %>
</div>


<% end  %>